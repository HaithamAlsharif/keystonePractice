<div class="container">
    <div class="row">
        <div class="col-md-12 col-lg-12">
            <div class="row">
                {{# each products}}
                    <div class="col-md-4 col-lg-4">
                        <h2><a href="{{productUrl slug}}">{{{title}}}</a></h2>
                        {{#if image}}
                            <img src="{{{cloudinaryUrl image crop='fit'}}}" class="thumbnail">
                        {{/if}}
                    </div>
                {{/each}}
            </div>
        </div>
    </div>
</div>